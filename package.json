{
  "name": "hittracker",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "git@github.com:lazerball/HitTracker.git",
  "author": "Johnny Robeson",
  "license": "AGPL-3.0",
  "scripts": {
    "lint:php-cs-fixer": "./vendor/bin/php-cs-fixer fix --diff --dry-run -v",
    "lint:php-analyze": "./vendor/bin/phpstan analyse --memory-limit='-1' -l7 -c phpstan.neon etc/ src/ bin/",
    "lint": "tslint --project . && npm run lint:php-cs-fixer && npm run lint:php-analyze",
    "encore-watch": "encore dev --watch",
    "build": "encore production",
    "bundle-third-party": "gulp bundle-third-party"
  },
  "devDependencies": {
    "@symfony/webpack-encore": "^0.20",
    "@types/gulp": "^4.0.5",
    "@types/jquery": "^3.3.9",
    "@types/tmp": "^0.0.33",
    "autoprefixer": "^8.6.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2017": "^6.24",
    "download": "^7.0",
    "fork-ts-checker-webpack-plugin": "^0.4.9",
    "gulp": "^3.9.1",
    "husky": "^1.0.0-rc.15",
    "node-sass": "^4.9.3",
    "postcss-loader": "^2.1.6",
    "prettier": "^1.14.3",
    "sass-loader": "^7.1.0",
    "tmp": "^0.0.33",
    "ts-loader": "^3.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.0",
    "typescript": "^3.0.3",
    "webpack-notifier": "^1.6"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-rc.11",
    "eventsource-polyfill": "^0.9",
    "font-awesome": "^4.7",
    "fs-jetpack": "^2.1.1",
    "jquery": "^3.3",
    "jquery-countdown": "^2.2",
    "lodash": "^4.17.11",
    "popper.js": "^1.14.4",
    "reflect-metadata": "^0.1.12",
    "tslib": "^1.9.3",
    "tslint-language-service": "^0.9.9",
    "vue": "^2.5.17",
    "vue-class-component": "^6.2",
    "vue-loader": "^14.2.3",
    "vue-property-decorator": "^7.0.0",
    "vue-template-compiler": "^2.5.17",
    "vue-trans": "0.0.3"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "printWidth": 120,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "browserslist": [
    "last 2 versions",
    "ios >= 8"
  ]
}
