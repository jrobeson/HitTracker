    "fork-ts-checker-webpack-plugin": "^1.3.7",
    "gulp": "^4.0.2",
    "husky": "^2.7.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "sass-loader": "^7.1.0",
    "tmp": "^0.1.0",
    "ts-loader": "^5.4.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2",
    "webpack-notifier": "^1.8.0"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "bootstrap": "^4.3.1",
