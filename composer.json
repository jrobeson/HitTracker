    "repositories":
    [
            {
                "type": "vcs",
                "url": "https://github.com/jrobeson/Resource"
            },

            {
                "type": "vcs",
                "url": "https://github.com/jrobeson/SyliusResourceBundle"
            },
            {
                "type": "vcs",
                "url": "https://github.com/jrobeson/SyliusUiBundle"
            }
    ],
