        "symfony/web-profiler-bundle": "^4.0",
        "twig/extensions": "^1.5",
        "vich/uploader-bundle": "^1.0",
        "vlucas/phpdotenv": "^2.0",
        "willdurand/hateoas-bundle": "^1.3",
