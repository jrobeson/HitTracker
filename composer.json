{
    "name": "lazerball/hit-tracker",
    "license": "AGPL-3.0",
    "type": "project",
    "description": "",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "LazerBall\\HitTracker\\": "src/"
        }
    },
    "require": {
        "php": ">=5.6",
        "symfony/symfony": "^2.7",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.3",
        "doctrine/migrations": "1.0.*@dev",
        "doctrine/doctrine-migrations-bundle": "2.1.*@dev",
        "twig/extensions": "^1.0",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "~2.6",
        "sensio/distribution-bundle": "^3.0",
        "sensio/framework-extra-bundle": "^3.0",
        "igorw/get-in": "^1.0",
        "nikic/iter": "^1.2",
        "friendsofsymfony/rest-bundle": "^1.5",
        "jms/serializer-bundle": "0.13.*",
        "sylius/resource-bundle": "^0.14",
        "c33s/static-page-content-bundle": "^2.0",
        "sylius/settings-bundle": "^0.14",
        "guzzlehttp/guzzle": "^5.0",
        "umpirsky/composer-permissions-handler": "^1.3",
        "nesbot/Carbon": "^1.18",
        "vlucas/phpdotenv": "2.*@dev",
        "nelmio/security-bundle": "dev-force-wdt-profiler-whitelist",
        "psy/psysh": "^0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.4",
        "phpunit/php-invoker": "^1.1",
        "behat/behat": "^3.0",
        "behat/symfony2-extension": "^2.0",
        "behat/mink-extension": "^2.0",
        "behat/mink-browserkit-driver": "^1.2",
        "behat/mink-selenium2-driver": "^1.2",
        "behat/mink": "^1.6",
        "doctrine/doctrine-fixtures-bundle": "^2.2",
        "fzaninotto/faker": "^1.2",
        "phpspec/phpspec": "^2.2",
        "matthiasnoback/symfony-config-test": "^1.0",
        "behat/mink-zombie-driver": "^1.2",
        "league/factory-muffin": "~3.0@beta",
        "league/factory-muffin-faker": "~1.0@beta"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "rm -f bin/doctrine bin/doctrine.php bin/doctrine-dbal",
            "Umpirsky\\PermissionsHandler\\ScriptHandler::setPermissions"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "rm -f bin/doctrine bin/doctrine.php bin/doctrine-dbal"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "preferred-install": "source"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "writable-dirs": [
            "var/cache",
            "var/logs"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jrobeson/NelmioSecurityBundle"
        }
    ]
}
