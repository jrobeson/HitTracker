        "igorw/get-in": "^1.0",
        "incenteev/hashed-asset-bundle": "^1.0",
        "guzzlehttp/guzzle": "^6.0",
        "jms/serializer-bundle": "^2.0",
        "jrobeson/sylius-settings-bundle": "^1.0@dev",
        "nelmio/security-bundle": "^2.0",
        "nesbot/Carbon": "^1.18",
        "nikic/iter": "^1.2",
