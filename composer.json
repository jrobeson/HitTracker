{
    "name": "lazerball/hit-tracker",
    "license": "AGPL-3.0",
    "type": "project",
    "description": "",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "HitTracker\\": "src/"
        }
    },
    "require": {
        "php": ">=5.6",
        "ext-pdo_pgsql": "*",
        "symfony/symfony": "~2.6",
        "doctrine/orm": "~2.5@dev",
        "doctrine/doctrine-bundle": "~1.3",
        "twig/extensions": "~1.0",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~3.0",
        "sensio/framework-extra-bundle": "~3.0",
        "igorw/get-in": "~1.0",
        "nikic/iter": "~1.1",
        "friendsofsymfony/rest-bundle": "~1.4",
        "jms/serializer-bundle": "0.12.*",
        "jms/di-extra-bundle": "~1.4",
        "elnur/bootstrap-bundle": "~0.1",
        "sylius/resource-bundle": "~0.1",
        "c33s/static-page-content-bundle": "~2.0",
        "kunstmaan/live-reload-bundle": "~2.3",
        "sylius/settings-bundle": "~0.11",
        "guzzlehttp/guzzle": "~4.2",
        "umpirsky/composer-permissions-handler": "~1.0",
        "nesbot/Carbon": "~1.12"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "phpunit/php-invoker": "~1.1",
        "behat/behat": "~3.0",
        "behat/symfony2-extension": "~2.0",
        "behat/mink-extension": "~2.0",
        "behat/mink-browserkit-driver": "~1.2",
        "behat/mink-selenium2-driver": "~1.2",
        "behat/mink": "~1.6",
        "doctrine/doctrine-fixtures-bundle": "2.2.*",
        "fzaninotto/faker": "~1.2",
        "phpspec/phpspec": "~2.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
             "rm -f bin/doctrine bin/doctrine.php bin/doctrine-dbal",
            "Umpirsky\\PermissionsHandler\\ScriptHandler::setPermissions"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
             "rm -f bin/doctrine bin/doctrine.php bin/doctrine-dbal"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "preferred-install": "source"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "writable-dirs": ["var/cache", "var/logs"]
    }
}
